{
	"manifest_version": 2,
	"name": "Youtube to MP3",
	"description": "Download Youtube in MP3 and customize Music Metadata",
	"version": "1.0",
	"browser_action": {
		"default_icon": "icons/icon.png",
		"default_popup": "popup.html"
	},
	"icons": { 
		"16": "icons/icon.png",
        "48": "icons/icon.png",
        "128": "icons/icon.png" 
	},
	"permissions": [
		"activeTab",
		"tabs",
		"storage",
		"downloads"
	]
}
